{
  "name": "zgl-todo",
  "description": "待办工具",
  "author": "zhaogl",
  "private": true,
  "version": "1.0.0",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron:dev": "concurrently \"vite dev\" \"wait-on http://127.0.0.1:3000 && electron .\"",
    "electron:build": "npm run build && electron-builder --config electron-builder.config.js",
    "electron:build:win": "npm run build && electron-builder --config electron-builder.config.js --win",
    "electron:build:mac": "npm run build && electron-builder --config electron-builder.config.js --mac",
    "electron:build:linux": "npm run build && electron-builder --config electron-builder.config.js --linux",
    "postinstall": "electron-builder install-app-deps",
    "debug:build": "node debug-build.js"
  },
  "dependencies": {
    "@ant-design/icons": "~6.1.0",
    "antd": "^6.2.1",
    "better-sqlite3": "^12.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.3",
    "@eslint/js": "^9.25.0",
    "@types/antd": "^1.0.4",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "concurrently": "^8.2.2",
    "electron": "^40.0.0",
    "electron-builder": "^24.13.3",
    "electron-log": "^5.4.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "fs-extra": "^11.3.3",
    "globals": "^16.0.0",
    "less": "^4.5.1",
    "prebuild-install": "^7.1.3",
    "rimraf": "^6.1.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "files": [
    "dist",
    "electron"
  ]
}
